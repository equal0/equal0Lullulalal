package client;

import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

import javax.swing.JFrame;
import javax.swing.JComboBox;
import java.awt.BorderLayout;
import javax.swing.JPanel;
import java.awt.FlowLayout;
import javax.swing.DefaultComboBoxModel;

public class insertUserEvaluations extends JFrame{
	private JComboBox location;
	private JComboBox food_type;
	private JComboBox average_score;
	
	public insertUserEvaluations() {
		this.setBounds(500, 200, 350, 370);
		this.setDefaultCloseOperation(this.EXIT_ON_CLOSE);
		getContentPane().setLayout(new BorderLayout(0, 0));
		North();
		
	}
	
	public void North(){
		JPanel panel = new JPanel();
		getContentPane().add(panel, BorderLayout.NORTH);
		panel.setLayout(new FlowLayout(FlowLayout.CENTER, 5, 5));
		
		location = new JComboBox();
		location.addItem("À§Ä¡");
		//location.setModel(new DefaultComboBoxModel(new String[] {"\uC704\uCE58"}));
		panel.add(location);
		
		food_type = new JComboBox();
		food_type.setModel(new DefaultComboBoxModel(new String[] {"\uC74C\uC2DD\uC885\uB958"}));
		panel.add(food_type);
		
		average_score = new JComboBox();
		average_score.setModel(new DefaultComboBoxModel(new String[] {"\uD3C9\uC810"}));
		panel.add(average_score);
		this.setVisible(true);
	}

	
	
	
	
	class Action implements ActionListener{

		@Override
		public void actionPerformed(ActionEvent e) {
			
		}
		
	}
	
	public static void main(String[] args) {
		new insertUserEvaluations();
	}
}
